{
  "name": "alunys/symfony-test-bundle",
  "description": "A personal test bundle",
  "type": "symfony-bundle",
  "authors": [
    {
      "name": "alunys",
      "email": "raphael.prmntr@gmail.com"
    }
  ],
  "license": [
    "MIT"
  ],
  "require": {
    "php": ">=7.1",
    "symfony/framework-bundle": "^4.0",
    "doctrine/orm": "^2.6.0",
    "doctrine/doctrine-bundle": "^1.6"
  },
  "require-dev": {
    "coduo/php-matcher": "^2.3",
    "doctrine/data-fixtures": "^1.3.0",
    "doctrine/doctrine-fixtures-bundle": "^3.0",
    "phpunit/phpunit": "^6.5",
    "symfony/yaml": "^4.0",
    "symfony/browser-kit": "^4.0"
  },
  "autoload": {
    "psr-4": {
      "Alunys\\TestBundle\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Alunys\\TestBundle\\Tests\\": "tests/testsuite"
    }
  }
}
